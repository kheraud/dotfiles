{
    "config": {
        "name": "Eteindre ?",
        "window_mode": 4,
        "lines": 3,
        "width": 400
    },
    "content": [
        {
            "title": "Suspendre l'activité",
            "icon": "system-hibernate",
            "command": "clipdel -d \".*\" && ~/.config/i3/lock-script && systemctl suspend"
        },
        {
            "title": "Eteindre",
            "icon": "system-shutdown",
            "command": "clipdel -d \".*\" && systemctl poweroff"
        },
        {
            "title": "Redémarrer",
            "icon": "system-reboot",
            "command": "clipdel -d \".*\" && systemctl reboot"
        }
    ]
}
